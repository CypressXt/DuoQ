<div class="row">
	<div class="col-md-6 col-md-offset-3 whiteContent">
		<div id="message">
			<%= debug @summoner %>
			<%= debug params %>
			<% if @message != nil %>
				<% @message.each do |key, value| %>
				<%= content_tag(:div, value, :class => "text-#{key}") %>
				<% @message={} %>
				<% end %>
			<% end %>
		</div>
	</div>
</div>