@font-face {
	font-family: 'Glyphicons Halflings';
	src: url(<%= asset_data_uri('glyphicons-halflings-regular.eot')%>);
	///assets/glyphicons-halflings-regular.eot
	src: url(<%= asset_data_uri('glyphicons-halflings-regular.eot')%>) format('embedded-opentype'),
	///assets/glyphicons-halflings-regular.eot?#iefix
	url(<%= asset_data_uri('glyphicons-halflings-regular.woff')%>) format('woff'),
	///assets/glyphicons-halflings-regular.woff
	url(<%= asset_data_uri('glyphicons-halflings-regular.ttf')%>) format('truetype'),
	///assets/glyphicons-halflings-regular.ttf
	url(<%= asset_data_uri('glyphicons-halflings-regular.svg')%>) format('svg');
	///assets/glyphicons-halflings-regular.svg#glyphicons-halflingsregular
}

.bs-docs-masthead{
	margin-top: 50px;
	h1{
		font-family: 'Open sans';
		font-size: 60px;
		font-weight: bold;
		line-height: 66px;
		margin-bottom: 10px;
		margin-top: 20px;
		text-shadow: rgb(8, 8, 8) 1px 1px 2px;
		color: rgb(255, 255, 255);
	}

	p{
		color: rgb(255, 255, 255);
		font-family: 'Open sans';
		font-size: 24px;
		font-weight: 300;
		line-height: 34px;
		margin-bottom: 10px;
		margin-left: 0px;
		margin-right: 0px;
		margin-top: 0px;
		text-shadow: rgb(8, 8, 8) 1px 1px 1px;
	}

	.header-text{
		padding: 3em;
	}
}

.navbar-brand{
	font-size: 25px;
}

.icon-bar{
	background-color: rgb(66, 139, 202);
}

.row{
	width: 99%;
	margin-left: auto;
	margin-right: auto;
}

.table{
	background-color: #525252;
	color: white;
}
